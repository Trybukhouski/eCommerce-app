{
  "name": "ecommerce-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development&&webpack serve --open",
    "dev": "set NODE_ENV=development&&webpack",
    "build": "set NODE_ENV=production&&webpack",
    "test": "jest --config=jest.config.mjs",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint --fix ./src",
    "format:check": "npx prettier --check ./src",
    "format": "npx prettier --write ./src",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,scss,less,json}": "npx prettier --write ./src "
  },
  "validate-branch-name": {
    "pattern": "^(feat|fix|init|docs|refactor){1}/RSS-ECOMM-\\d{1,2}_C?\\d{2,4}/[a-zA-Z0-9-]*$",
    "errorMsg": "Illegal branch name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.9.0",
    "module-alias": "^2.2.3",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.9",
    "prettier": "^2.0.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "svg-sprite-loader": "^6.0.11",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "validate-branch-name": "^1.3.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  }
}
